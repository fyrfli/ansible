      community.general.pushover:
        app_token: "{{ lookup('env','PUSHOVER_APP_ID') }}"
        user_key: "{{ lookup('env','PUSHOVER_USER_ID') }}"
        msg: "From ansible-semaphore: Hi! "
        pri: 0
      delegate_to: localhost
