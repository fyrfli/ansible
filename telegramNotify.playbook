---
- hosts: live
  tasks:
    - name: Send notify to Telegram
      community.general.telegram:
        token: "{{ lookup('env','TELEGRAM_BOT_ID') }}"
        api_args:
          chat_id: "{{ lookup('env','MY_TELEGRAM_ID') }}"
          parse_mode: "markdown"
          text: "Hello from anible"
          disable_web_page_preview: true
          disable_notification: true
